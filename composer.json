{
    "name": "zendframework/zend-debug",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "debug"
    ],
    "homepage": "https://github.com/zendframework/zend-debug",
    "autoload": {
        "psr-4": {
            "Zend\\Debug\\": "src/"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0"
        "zendframework/zend-escaper": "2.*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "^4.8"
    },
    "suggest": {
        "ext-xdebug": "XDebug, for better backtrace output"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev",
            "dev-develop": "2.6-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Debug\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "php-cs-fixer --version && php-cs-fixer fix -v --diff --dry-run",
        "cs-fix": "php-cs-fixer fix -v",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}
